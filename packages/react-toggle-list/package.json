{
  "name": "react-toggle-list",
  "version": "1.0.0",
  "description": "Provides easy toggle list with React.",
  "main": "dist/react-toggle-list.cjs.js",
  "module": "dist/react-toggle-list.esm.js",
  "types": "dist/react-toggle-list.cjs.d.ts",
  "sideEffects": false,
  "author": "in-ch",
  "license": "MIT",
  "repository": "https://github.com/in-ch/react-file-folder.git",
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "react-icons": "^5.0.1",
    "tailwindcss": "^3.4.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "*"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "next",
    "reactjs",
    "nextjs",
    "toggle",
    "toggle list",
    "toggle-list",
    "ui",
    "component"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "exports": {
    ".": {
      "module": "./dist/react-toggle-list.esm.js",
      "import": "./dist/react-toggle-list.cjs.mjs",
      "default": "./dist/react-toggle-list.cjs.js"
    },
    "./package.json": "./package.json"
  }
}
